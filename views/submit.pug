extends layout

block content
    .form-container
        form(action=routes.submit, method ="post")
            input(type="name", name="name", placeholder="이름",required=true)
            input(type="string", name="phone", placeholder="번호",required=true)
            input(type="number", name="temperature", placeholder="체온",required=true)
            input(type="submit", value="제출")